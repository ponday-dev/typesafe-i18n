{
  "compilerOptions": {
    "target": "es5",
    "noEmit": true,
    "module": "esnext",
    "moduleResolution": "node",
    "lib": ["dom", "esnext"],
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "declaration": true,
    "declarationDir": "lib"
  },
  "include": ["src/**/*.ts"]
}
